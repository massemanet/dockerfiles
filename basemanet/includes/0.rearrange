copy bashrc              "$HOME/.bashrc"
copy gitconfig           "$HOME/.gitconfig"
copy gitfunctions.sh     "$HOME/gitfunctions.sh"
copy /tmp/gitconfig      "$HOME/.gitconfig"
link /tmp/.vscode        "$HOME/.vscode"
link /tmp/.aws           "$HOME/.aws"
link /tmp/.kube          "$HOME/.kube"
link /tmp/.ssh           "$HOME/.ssh"
link /root/.emacs.d      "$HOME/.emacs.d"
link /tmp/.vscode/config "$HOME/.config/Code"
